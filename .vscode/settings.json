{
  "jest.rootPath": ".github/scripts",
  "jest.jestCommandLine": "npm test --",
  "jest.enableInlineErrorMessages": true,
  "jest.autoRun": "off",
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "files.exclude": {
    "node_modules/**": true,
    "src/**/*.js": true,
    "src/**/*.js.map": true,
    "src/**/*.d.ts": true,
    "packages/**/*.js.map": true,
    "packages/**/*.d.ts": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "packages/rulesets/spectral/functions/*.js":false,
  },
  "[markdown]": {
    "editor.formatOnSave": false,
    "editor.formatOnPaste": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.exclude": {
    "**/dist/**": true,
    "**/dist-dev/**": true,
    "**/CHANGELOG.json": true,
    "**/CHANGELOG.md": true,
    "docs/spec.html": true,
    "common/config/rush/pnpm-lock.yaml": true
  },
  "typescript.tsdk": "./common/temp/node_modules/typescript/lib",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "cSpell.words": [
    "dataplane",
    "indexof",
    "Kusto",
    "openapi",
    "petstore",
    "regen",
    "ruleindex",
    "ruleset",
    "rulesets",
    "startswith",
    "strcat",
    "tostring",
    "yzhang"
  ]
}
